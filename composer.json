{
  "name": "libarea/docs",
  "description": "Simple documentation that uses Markdown files for a project.",
  "type": "project",
  "keywords": [
    "hleb",
    "libarea",
    "documentation",
    "wiki",
    "mvc",
    "php"
  ],
  "homepage": "https://github.com/libarea/docs",
  "license": "MIT",
  "authors": [
    {
      "name": "Evgeny Konchik",
      "email": "dev@libarea.ru",
      "homepage": "https://libarea.ru"
    }
  ],
  "require": {
    "php": ">=7.4.0",
    "phphleb/framework": ">=1.6.70",
    "phphleb/imageresizer": "dev-master",
    "erusev/parsedown": "^1.8.0@beta"
  },
  "autoload": {
    "classmap": [
      "app/",
      "vendor/phphleb"
    ],
    "psr-4": {
      "App\\": "app/",
      "Phphleb\\": "vendor/phphleb/",
      "Hleb\\": "vendor/phphleb/framework/"
    }
  },
  "config": {
    "optimize-autoloader": true
  },
  "minimum-stability": "dev"
}
